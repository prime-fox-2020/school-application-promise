<%- include('partials/header', {title: 'Teachers Page'}) %>

<div class="container">
  <h1>Teachers Page</h1>
  <br><br>
  <table class="table table-striped">
    <thead>
      <tr>
        <tr>
          <th scope="col">Id</th>
          <th scope="col">First Name</th>
          <th scope="col">Last Name</th>
          <th scope="col">Email</th>
          <th scope="col">Gender</th>
        </tr>
      </tr>
    </thead>
    <tbody>
      <% data.forEach(el => { %>
        <tr>
          <td><%= el.id %></td>
          <td><%= el.first_name %></td>
          <td><%= el.last_name %></td>
          <td><%= el.email %></td>
          <td><%= el.gender %></td>
        </tr>
      <% }) %>
    </tbody>
  </table>
</div>

<%- include('partials/footer') %>